<p>Name: <%= @attraction.name %></p>
<p>Total Number of Users: <%= @attraction.name %></p>
<p>Ticket Requirement: <%= @attraction.tickets %></p>
<p>Height Requirement: <%= @attraction.min_height %></p>
<p>Nausea Rating: <%= @attraction.nausea_rating %></p>
<p>Happiness Rating: <%= @attraction.happiness_rating %></p>
<% if current_user.admin %>
    <p><%= link_to "Edit Attraction", edit_attraction_path(@attraction) %></p>
<% else %>
    <%= form_for @ride do |f| %>
    <%= f.hidden_field :user_id %>
    <%= f.hidden_field :attraction_id %>
    <%= f.submit "Go on this ride" %>
    <% end %>
<% end %>